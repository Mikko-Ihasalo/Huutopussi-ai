from . import game_mechanic_classes as gmc
from . import game_mechanic_functions as gmf
from . import game_engine as ge

def test_bidding_round():
    errors = []
    if len(errors) > 0:
        for error in errors:
            print(error)
        print(f"errors: {len(errors)}")
    assert len(errors) == 0

def test_play_round(): 
    errors = []
    if len(errors) > 0:
        for error in errors:
            print(error)
        print(f"errors: {len(errors)}")
    assert len(errors) == 0
    
